<table class="">
    <thead>
        <tr>
            <th>Who</th>
            <th>When</th>
            <th>What</th>
        </tr>
    </thead>
    <tbody>
{% for s in data_array %}
<tr>
    <td>{% if s.username is defined %}{{ s.username }}{% else %}&nbsp;{% endif %}</td>
    <td>{% if s.logged_at is defined %}{{ s.logged_at|date('Y-m-d H:i') }}{% else %}&nbsp;{% endif %}</td>
    <td>{% if s.data is defined %}{{ s.data | prettyprint }}{% else %}&nbsp;{% endif %}</td>
</tr>
{% endfor %}
</tbody>
</table>
